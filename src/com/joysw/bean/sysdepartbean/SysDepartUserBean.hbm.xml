<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.joysw.bean.sysdepartbean.SysDepartUserBean"
		table="sys_depart_user">
		<composite-id>
			<key-property name="departId" column="depart_id" type="java.lang.Integer" />
			<key-property name="userId" column="user_id" type="java.lang.Integer" />
		</composite-id>
	</class>
</hibernate-mapping>
