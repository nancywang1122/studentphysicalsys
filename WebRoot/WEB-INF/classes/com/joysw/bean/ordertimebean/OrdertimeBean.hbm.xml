<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.joysw.bean.ordertimebean.OrdertimeBean" table="tb_order_time">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="year" type="java.lang.Integer">
			<column name="year"></column>
		</property>
		<property name="month" type="java.lang.Integer">
			<column name="month"></column>
		</property>

		<property name="day" type="java.lang.Integer">
			<column name="day"></column>
		</property>

		<property name="week" type="java.lang.Integer">
			<column name="week"></column>
		</property>

		<property name="weekno" type="java.lang.Integer">
			<column name="weekno"></column>
		</property>
	</class>
</hibernate-mapping>